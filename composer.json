{
    "name": "foleybridgesolutions/kotapay-cashier",
    "description": "Laravel Cashier integration for Kotapay ACH payment gateway",
    "version": "1.0.0",
    "keywords": ["laravel", "cashier", "kotapay", "ach", "billing", "payment"],
    "license": "MIT",
    "authors": [
        {
            "name": "FoleyBridge Solutions",
            "email": "dev@foleybridgesolutions.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "nesbot/carbon": "^2.0|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "FoleyBridgeSolutions\\KotapayCashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FoleyBridgeSolutions\\KotapayCashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FoleyBridgeSolutions\\KotapayCashier\\CashierServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
